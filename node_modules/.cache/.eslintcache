],"/Users/shannasmith/code/projects/watchlist/src/context/AppReducer.jsx",["58"],[],"export default (state, action) => {\n  switch (action.type) {\n    case 'ADD_MOVIE_TO_WATCHLIST':\n      return {\n        ...state, //return existing state\n        watchlist: [action.payload, ...state.watchlist], //changes to the state. new array with the movie data added to existing state\n      };\n    case 'REMOVE_MOVIE_FROM_WATCHLIST':\n      return {\n        ...state,\n        // return all the movies that are not equal to the id passed\n        watchlist: state.watchlist.filter(\n          (movie) => movie.id !== action.payload\n        ),\n      };\n    case 'ADD_MOVIE_TO_WATCHED':\n      return {\n        ...state,\n        watchlist: state.watchlist.filter(\n          (movie) => movie.id !== action.payload.id\n        ),\n        watched: [action.payload, ...state.watched],\n      };\n    case 'MOVE_TO_WATCHLIST':\n      return {\n        ...state,\n        // remove movie from watched\n        watched: state.watched.filter(\n          // action.payload.id is passed because we need the whole movie\n          (movie) => movie.id !== action.payload.id\n        ),\n        // add movie back to watchlist\n        watchlist: [action.payload, ...state.watchlist],\n      };\n    case 'REMOVE_FROM_WATCHED':\n      return {\n        ...state,\n        watched: state.watched.filter((movie) => movie.id !== action.payload),\n      };\n    default:\n      return state;\n  }\n};\n","/Users/shannasmith/code/projects/watchlist/src/components/MovieControls.jsx",[],[],{"ruleId":"59","severity":1,"message":"60","line":1,"column":1,"nodeType":"61","endLine":43,"endColumn":3},"import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration"]